{
  "name": "portfinder",
  "description": "A simple tool to find an open port on the current machine",
  "version": "1.0.21",
  "author": "Charlie Robbins <charlie.robbins@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:indexzero/node-portfinder.git"
  },
  "keywords": [
    "http",
    "ports",
    "utilities"
  ],
  "files": [
    "lib"
  ],
  "dependencies": {
    "async": "2.6.2",
    "debug": "4.1.1",
    "mkdirp": "0.5.1"
  },
  "devDependencies": {
    "ava": "1.4.1",
    "glob": "7.1.3",
    "nyc": "14.1.0",
    "vows": "0.8.2"
  },
  "main": "./lib/portfinder",
  "types": "./lib/portfinder.d.ts",
  "scripts": {
    "test": "vows test/*-test.js --spec",
    "wip": "NODE_ENV=dev nyc -x='**/examples/*' -x='**/dist/*' -x='**/benchmark/*' -x='**/*balanced*.js' -x='**/test/*.js' -x='**/lib/*.js' ava --timeout=60000"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "license": "MIT"
}
